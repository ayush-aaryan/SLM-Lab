        },
        "critic_optim_spec": {
          "name": "Adam",
          "lr": 2.5e-4,
        },
        "gpu": false
      }
    }],
    "env": [{
      "name": "aapl_env-v0",
      "frame_op": null,
      "frame_op_len": 1,
